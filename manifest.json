{
  "name": "Crypto Micropayments",
  "version": "0.1",
  "description": "Put some money where your mind is",
  "author": "jpchou",
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16.png",
      "32": "images/icon32.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  },
  "background": {
    "scripts": ["js/jquery-3.4.1.min.js", "js/oauth.js"],
    "persistent": false
  },
  "permissions": [
    "identity",
    "storage",
    "notifications",
    "https://api.coinbase.com/*"
  ],
  "manifest_version": 2
}
